        "popper.js": "^1.16.1",
        "vue": "^2.6.11"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.13.1",
        "admin-lte": "^3.0.5",
        "ajv": "^6.12.2",
        "laravel-vue-pagination": "^2.1.0",
        "moment": "^2.27.0",
        "sweetalert2": "^7.26.12",
        "vform": "^1.0.0",
        "vue-progressbar": "^0.7.5",
        "vue-router": "^3.3.4"
